			<data key="w">0.3</data>
		</edge>
		<edge id="1" source="3" target="4">
			<data key="w">0.1</data>
		</edge>
		<edge id="2" source="4" target="5">
			<data key="w">0.6</data>
		</edge>
		<edge id="3" source="1" target="5">
			<data key="w">0.8</data>
		</edge>
		<edge id="4" source="2" target="5">
			<data key="w">0.1</data>
		</edge>
		<edge id="5" source="5" target="7">
			<data key="w">0</data>
		</edge>
		<edge id="6" source="8" target="9">
			<data key="w">1.0</data>
		</edge>
		<edge id="7" source="4" target="8">
			<data key="w">0.49999</data>
		</edge>
		<edge id="8" source="4" target="6">
			<data key="w">0.50001</data>
		</edge>
		<edge id="9" source="3" target="9">
			<data key="w">0.5</data> <!-- an edge case given cutoff=0.5 -->
		</edge>
